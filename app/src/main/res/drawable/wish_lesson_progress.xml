<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">
            <corners android:radius="10dp" /> <!-- 둥근 테두리 -->
            <solid android:color="@color/light_gray_2" /> <!-- 배경 색상 -->
        </shape>
    </item>

    <item>
        <clip>
            <shape android:shape="rectangle">
                <corners android:radius="10dp" /> <!-- 둥근 테두리 -->
                <solid android:color="#01A76B" /> <!-- 프로그래스바 색상 -->
            </shape>
        </clip>
    </item>
</layer-list>